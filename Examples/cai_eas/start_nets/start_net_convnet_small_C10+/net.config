{
    "name": "SimpleConvnet",
    "weight_decay": 0.0001,
    "bn_epsilon": 1e-05,
    "bn_decay": 0.9,
    "drop_scheme": {
        "type": "conv",
        "conv_drop": 1.0,
        "pool_drop": 0.7,
        "fc_drop": 0.5
