            {
                "name": "fc",
                "units": 8,
                "use_bias": false,
                "_id": "fc_0",
                "use_bn": true,
                "activation": "relu",
                "keep_prob": 1.0,
                "pre_activation": false
            },
            {
                "name": "fc",
                "units": 10,
                "use_bias": true,
                "_id": "fc_1",
                "use_bn": false,
                "activation": null,
                "keep_prob": 1.0,
                "pre_activation": false
            }
        ]
    }
